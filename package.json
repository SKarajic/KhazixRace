{
  "name": "custom-server-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "nest start -w -p tsconfig.server.json",
    "predev": "yarn typeorm migration:run",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node -r ./build/paths.server.js .nest/server/main.js",
    "prestart": "yarn typeorm migration:run",
    "cli": "ts-node -r tsconfig-paths/register --project tsconfig.server.json server/console.ts",
    "typeorm": "ts-node -r tsconfig-paths/register --project tsconfig.server.json ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn typeorm migration:generate -d migrations -n"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^7.1.0",
    "@nestpress/next": "^1.2.3",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "commander": "^5.1.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "kayn": "^0.10.2",
    "nestjs-console": "^3.0.5",
    "next": "latest",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prompt": "^1.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-feather": "^2.0.8",
    "react-twitch-embed-video": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/react": "^16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "add": "^2.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.4",
    "yarn": "^1.22.4"
  }
}
